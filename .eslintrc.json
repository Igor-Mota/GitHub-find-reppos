{
  "parser": "babel-eslint",

    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "react/prop-types": 0,
      "react/jsx-filename-extension":[
        "error",
        {
          "extensions":[".js", ".jsx"]
        }
      ]
    }
}
